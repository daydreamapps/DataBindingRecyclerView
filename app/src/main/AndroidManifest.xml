<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.daydreamapplications.sample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity android:name="com.daydreamapplications.sample.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".MultipleItemTypesActivity" />
        <activity android:name=".SingleItemTypeActivity" />
        <activity android:name=".SingleItemTypeAsyncActivity" />
        <activity android:name=".DiffUtilSortActivity" />
        <activity android:name=".DiffUtilAddRemoveActivity" />

    </application>

</manifest>